{
  "name": "web-dan-for-js",
  "version": "1.13.3",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration production --base-href \"https://structuralengine.github.io/WebDanforJS/\" && angular-cli-ghpages",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:dev": "concurrently \"ng build --delete-output-path=false --watch\" \"webpack --config webpack.electron.dev.config.js --watch\"",
    "start:dev": "electron dist/shell.js",
    "electron:windows": "set NODE_OPTIONS=--openssl-legacy-provider && npm run build:prod && npx electron-builder build --windows",
    "build:prod": "ng build --configuration production --optimization --outputHashing=all --sourceMap=false --namedChunks=false --extract-licenses --vendorChunk=false --build-optimizer && webpack --config webpack.electron.prod.config.js && npm run filecopy1 && npm run filecopy2",
    "start:prod": "electron dist/index.js",
    "filecopy1": "node -e \"require('fs').copyFileSync('dev-app-update.yml', 'dist/dev-app-update.yml')\"",
    "filecopy2": "node -e \"require('fs').copyFileSync('app-update.yml', 'dist/app-update.yml')\""
  },
  "description": "WebDanforJS",
  "author": {
    "name": "sasaco",
    "email": "sasaicco@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 6",
    "typescript",
    "sass"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~12.2.0",
    "@angular/localize": "~12.2.0",
    "@angular/material": "^12.2.8",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/encoding-japanese": "^2.0.1",
    "@types/file-saver": "^2.0.3",
    "@types/jquery": "^3.5.6",
    "@types/jqueryui": "^1.12.16",
    "angular-cli-ghpages": "^1.0.0",
    "bootstrap": "^4.5.0",
    "electron-is-dev": "^2.0.0",
    "electron-nsis": "0.0.1-alpha",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.6.5",
    "encoding-japanese": "^1.0.30",
    "file-saver": "^2.0.5",
    "firebase": "^8.3.1",
    "jquery": "^3.6.0",
    "jquery-ui-pack": "^1.12.3",
    "jszip": "^2.5.0",
    "ngx-print": "^1.2.1",
    "pqgrid": "file:./paramquery-8.4.0/",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "webpack": "^5.75.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1200.0",
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular/cli": "~12.2.7",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^14.15.5",
    "concurrently": "^6.2.1",
    "electron": "^13.2.0",
    "electron-builder": "^22.14.13",
    "electron-log": "4.4.1",
    "electron-packager": "^15.3.0",
    "firebase-tools": "^9.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.8.0",
    "jsonc-parser": "^3.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ngx-electron": "^2.2.0",
    "open": "^7.0.3",
    "ts-loader": "^9.2.5",
    "typescript": "~4.3.5",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
