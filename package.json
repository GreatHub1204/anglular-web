{
  "name": "web-dan-for-js",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --configuration production --base-href \"https://structuralengine.github.io/WebDanforJS/\" && angular-cli-ghpages",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",

    "build:dev": "concurrently \"ng build --delete-output-path=false --watch\" \"webpack --config webpack.electron.dev.config.js --watch\"",
    "start:dev": "electron dist/shell.js",
    "build:prod": "ng build --optimization --outputHashing=all --sourceMap=false --namedChunks=false --extract-licenses --vendorChunk=false --build-optimizer && webpack --config webpack.electron.prod.config.js",
    "start:prod": "electron dist/main.js"
  },
  "description": "断面照査プログラム",
  "author": {
    "name": "sasaco",
    "email": "sasaicco@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 6",
    "typescript",
    "sass"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~12.2.0",
    "@angular/localize": "~12.2.0",
    "@angular/material": "^12.2.8",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/file-saver": "^2.0.3",
    "@types/jquery": "^3.5.6",
    "@types/jqueryui": "^1.12.16",
    "bootstrap": "^4.5.0",
    "encoding-japanese": "^1.0.30",
    "file-saver": "^2.0.5",
    "firebase": "^8.3.1",
    "jquery": "^3.6.0",
    "jquery-ui-pack": "^1.12.3",
    "jszip": "^3.7.1",
    "ngx-print": "^1.2.1",
    "pqgrid": "^8.1.0",
    "rxfire": "^6.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
